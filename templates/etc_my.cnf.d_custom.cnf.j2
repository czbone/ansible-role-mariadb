# {{ ansible_managed }}
#
# MariaDB Custom server configuration
#

{% for section_name, section_contents in mariadb_custom_cnf.iteritems() %}
[{{ section_name }}]
{% for key, value in section_contents.iteritems() %}
{% if value is defined and value %}
{{ key }}={{ value }}
{% else %}
{{ key }}
{% endif %}
{% endfor %}

{% endfor %}
